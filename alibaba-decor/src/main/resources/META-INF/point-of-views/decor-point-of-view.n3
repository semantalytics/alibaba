# -----------------------------------------------
# Individual LiteralDisplays
# -----------------------------------------------

rdfs:label a owl:DatatypeProperty.
ali:label a pov:LiteralDisplay;
	rdfs:label "Label"@en;
	rdfs:comment "Name or title of the resource."@en;
	pov:properties [a rdf:Alt; rdf:_1 rdfs:label];
	pov:style ali:normal;
	pov:format ali:none;
	pov:name "label".

rdfs:comment a owl:DatatypeProperty.
ali:comment a pov:LiteralDisplay;
	rdfs:label "Comment"@en;
	rdfs:comment "Description or note about the resource."@en;
	pov:properties [a rdf:Alt; rdf:_1 rdfs:comment];
	pov:style ali:normal;
	pov:format ali:none;
	pov:name "comment".

dcr:before a owl:DatatypeProperty.
ali:before a pov:LiteralDisplay;
	rdfs:label "Before"@en;
	rdfs:comment "Printed before the value."@en;
	pov:properties [a rdf:Alt; rdf:_1 dcr:before];
	pov:style ali:normal;
	pov:format ali:none;
	pov:name "before".

dcr:after a owl:DatatypeProperty.
ali:after a pov:LiteralDisplay;
	rdfs:label "After"@en;
	rdfs:comment "Printed after the value."@en;
	pov:properties [a rdf:Alt; rdf:_1 dcr:after];
	pov:style ali:normal;
	pov:format ali:none;
	pov:name "after".

dcr:separation a owl:DatatypeProperty.
ali:separation a pov:LiteralDisplay;
	rdfs:label "Separation"@en;
	rdfs:comment "Printed inbetween the values."@en;
	pov:properties [a rdf:Alt; rdf:_1 dcr:separation];
	pov:style ali:normal;
	pov:format ali:none;
	pov:name "separation".

dcr:empty a owl:DatatypeProperty.
ali:empty a pov:LiteralDisplay;
	rdfs:label "Empty"@en;
	rdfs:comment "Printed if there are no values."@en;
	pov:properties [a rdf:Alt; rdf:_1 dcr:empty];
	pov:style ali:normal;
	pov:format ali:none;
	pov:name "empty".

