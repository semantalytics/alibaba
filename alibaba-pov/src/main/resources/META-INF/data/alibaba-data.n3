# -----------------------------------------------
# Individual MessagePatternFormats
# -----------------------------------------------

ali:hidden a pov:MessagePatternFormat;
    rdfs:label "hidden"@en;
    rdfs:comment "No values are shown."@en;
    pov:pattern "".

ali:second-first a pov:MessagePatternFormat;
    rdfs:label "lastFirst"@en;
    rdfs:comment "The last value is displayed before the first separated by a comma."@en;
    pov:pattern "{1}, {0}".

ali:secret a pov:MessagePatternFormat;
    rdfs:label "secret"@en;
    rdfs:comment "Hides the value by replacing it with '*' charaters."@en;
    pov:pattern "********".

ali:short-dateTime a pov:MessagePatternFormat;
    rdfs:label "short dateTime"@en;
    rdfs:comment "Displays the dateTime in the short style."@en;
    pov:pattern "{0,date,short} {0,time,short}".

ali:medium-dateTime a pov:MessagePatternFormat;
    rdfs:label "medium dateTime"@en;
    rdfs:comment "Displays the dateTime in the medium style."@en;
    pov:pattern "{0,date,medium} {0,time,medium}".

ali:long-dateTime a pov:MessagePatternFormat;
    rdfs:label "long dateTime"@en;
    rdfs:comment "Displays the dateTime in the long style."@en;
    pov:pattern "{0,date,long} {0,time,long}".

ali:full-dateTime a pov:MessagePatternFormat;
    rdfs:label "full dateTime"@en;
    rdfs:comment "Displays the dateTime in the full style."@en;
    pov:pattern "{0,date,full} {0,time,full}".

ali:short-date a pov:MessagePatternFormat;
    rdfs:label "short date"@en;
    rdfs:comment "Displays the date in the short style."@en;
    pov:pattern "{0,date,short}".

ali:medium-date a pov:MessagePatternFormat;
    rdfs:label "medium date"@en;
    rdfs:comment "Displays the date in the medium style."@en;
    pov:pattern "{0,date,medium}".

ali:long-date a pov:MessagePatternFormat;
    rdfs:label "long date"@en;
    rdfs:comment "Displays the date in the long style."@en;
    pov:pattern "{0,date,long}".

ali:full-date a pov:MessagePatternFormat;
    rdfs:label "full date"@en;
    rdfs:comment "Displays the date in the full style."@en;
    pov:pattern "{0,date,full}".

ali:short-time a pov:MessagePatternFormat;
    rdfs:label "short time"@en;
    rdfs:comment "Displays the time in the short style."@en;
    pov:pattern "{0,time,short}".

ali:medium-time a pov:MessagePatternFormat;
    rdfs:label "medium time"@en;
    rdfs:comment "Displays the time in the medium style."@en;
    pov:pattern "{0,time,medium}".

ali:long-time a pov:MessagePatternFormat;
    rdfs:label "long time"@en;
    rdfs:comment "Displays the time in the long style."@en;
    pov:pattern "{0,time,long}".

ali:full-time a pov:MessagePatternFormat;
    rdfs:label "full time"@en;
    rdfs:comment "Displays the time in the full style."@en;
    pov:pattern "{0,time,full}".

ali:decimal a pov:MessagePatternFormat;
    rdfs:label "decimal"@en;
    rdfs:comment "Formats the value as a decimal."@en;
    pov:pattern "{0,number}".

ali:integer a pov:MessagePatternFormat;
    rdfs:label "integer"@en;
    rdfs:comment "Formats the value as an integer."@en;
    pov:pattern "{0,number,integer}".

ali:scientific a pov:MessagePatternFormat;
    rdfs:label "scientific"@en;
    rdfs:comment "Formats the value a scientific floating point number."@en;
    pov:pattern "{0,number,0.#####E0}".

ali:percent a pov:MessagePatternFormat;
    rdfs:label "percent"@en;
    rdfs:comment "Formats the value as a percentage of 1."@en;
    pov:pattern "{0,number,percent}".

ali:currency a pov:MessagePatternFormat;
    rdfs:label "currency"@en;
    rdfs:comment "Formats the value in the local currency."@en;
    pov:pattern "{0,number,currency}".

