@prefix rdf:        <http://www.w3.org/1999/02/22-rdf-syntax-ns#>.
@prefix rdfs:       <http://www.w3.org/2000/01/rdf-schema#>.
@prefix dc:         <http://purl.org/dc/elements/1.1/>.
@prefix xsd:        <http://www.w3.org/2001/XMLSchema#>.
@prefix owl:        <http://www.w3.org/2002/07/owl#>.
@prefix pov:        <http://www.openrdf.org/rdf/2007/08/point-of-view#>.
@prefix ali:        <http://www.openrdf.org/rdf/2007/08/alibaba/>.

# -----------------------------------------------
# Individuals
# -----------------------------------------------

# -----------------------------------------------
# Individual PresentationRepositories
# -----------------------------------------------

ali:presentation-repository a pov:PresentationRepository;
    rdfs:label "presentation repository"@en;
    rdfs:comment "The main presentation repository."@en;
    pov:registeredPresentation ali:text-presentation;
    pov:registeredPresentation ali:json-presentation;
    pov:registeredPresentation ali:html-presentation;
    pov:registeredPresentation ali:xul-presentation.

# -----------------------------------------------
# Individual Presentations
# -----------------------------------------------

ali:text-presentation a pov:TextPresentation;
    rdfs:label "text presentation"@en;
    rdfs:comment "Presentation for Plain text display."@en;
    pov:accept "text/plain";
    pov:accept "text/plain;q=0.8";
    pov:contentType "text/plain";
    pov:intention ali:present;
    pov:intention ali:general;
    pov:intention ali:detail;
    pov:intention ali:related;
    pov:encoding ali:none;
    pov:representations ali:text-representation-repository;
    pov:searchPatterns ali:search-pattern-repository;
    pov:perspectives ali:perspective-repository;
    pov:formats ali:format-repository;
    pov:perspectiveFactory ali:perspective-factory;
    pov:decoration ali:none.

ali:json-presentation a pov:TextPresentation;
    rdfs:label "json presentation"@en;
    rdfs:comment "Presentation for JavaScript Object Notation."@en;
    pov:accept "text/json";
    pov:accept "application/json";
    pov:contentType "text/json";
    pov:intention ali:general;
    pov:intention ali:detail;
    pov:intention ali:related;
    pov:encoding ali:javascript-encoding;
    pov:representations ali:json-representation-repository;
    pov:searchPatterns ali:search-pattern-repository;
    pov:perspectives ali:perspective-repository;
    pov:formats ali:format-repository;
    pov:perspectiveFactory ali:perspective-factory;
    pov:decoration ali:none.

ali:html-presentation a pov:TextPresentation;
    rdfs:label "HTML presentation"@en;
    rdfs:comment "Presentation for HTML display."@en;
    pov:accept "*/*";
    pov:accept "*/*;q=0.5";
    pov:accept "text/html";
    pov:accept "text/html;q=0.9";
    pov:accept "application/xhtml+xml";
    pov:contentType "text/html";
    pov:intention ali:present;
    pov:intention ali:general;
    pov:intention ali:detail;
    pov:intention ali:related;
    pov:encoding ali:xml-encoding;
    pov:representations ali:html-representation-repository;
    pov:searchPatterns ali:search-pattern-repository;
    pov:perspectives ali:perspective-repository;
    pov:formats ali:format-repository;
    pov:perspectiveFactory ali:perspective-factory;
    pov:decoration [a pov:TextDecoration;
        pov:before """<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" 
        "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml">
<head>
  <title>Alibaba</title>
  <meta name="description" content="" />
  <meta name="keywords" content="" />
  <style type="text/css">
    a.representation {
      margin: 0px 0.5em;
    }
    a.representation span.value {
      margin: 0px 0px;
    }
    span.value {
      margin: 0px 0.5em;
    }
  </style>
</head>
<body>
""";
        pov:after "\n</body></html>"].

ali:xul-presentation a pov:TextPresentation;
    rdfs:label "XUL presentation"@en;
    rdfs:comment "Presentation for XUL display."@en;
    pov:accept "text/xul";
    pov:accept "application/vnd.mozilla.xul+xml";
    pov:contentType "application/vnd.mozilla.xul+xml";
    pov:intention ali:present;
    pov:intention ali:general;
    pov:intention ali:detail;
    pov:intention ali:related;
    pov:encoding ali:xml-encoding;
    pov:representations ali:xul-representation-repository;
    pov:searchPatterns ali:search-pattern-repository;
    pov:perspectives ali:perspective-repository;
    pov:formats ali:format-repository;
    pov:perspectiveFactory ali:perspective-factory;
    pov:decoration [a pov:TextDecoration;
        pov:before """<?xml version="1.0"?>
<?xml-stylesheet href="chrome://global/skin/" type="text/css"?>
<window xmlns:html="http://www.w3.org/1999/xhtml"
  xmlns="http://www.mozilla.org/keymaster/gatekeeper/there.is.only.xul">
  <scrollbox flex='1' style='overflow: auto;' orient='vertical'>
""";
        pov:after "\n</scrollbox></window>"].

# -----------------------------------------------
# Individual Decorations
# -----------------------------------------------

ali:none a pov:Decoration.

# -----------------------------------------------
# Individual TextDecorations
# -----------------------------------------------

ali:none a pov:TextDecoration.

# -----------------------------------------------
# Individual Encodings
# -----------------------------------------------

ali:xml-encoding a pov:Encoding;
    rdfs:label "xml encoding"@en;
    rdfs:comment "Convents '<' and '>' into '&lt;' and '&gt;'."@en.

ali:javascript-encoding a pov:Encoding;
    rdfs:label "javascript encoding"@en;
    rdfs:comment "Escapes characters into a javascript string."@en.

ali:none a pov:Encoding.

# -----------------------------------------------
# Individual RepresentationRepositories
# -----------------------------------------------

ali:text-representation-repository a pov:RepresentationRepository;
    rdfs:label "text representation repository"@en;
    rdfs:comment "Representation repository for plain text representations."@en;
    pov:registeredRepresentation ali:text-table-representation;
    pov:registeredRepresentation ali:text-inline-representation.

ali:json-representation-repository a pov:RepresentationRepository;
    rdfs:label "json representation repository"@en;
    rdfs:comment "Representation repository for JavaScript Object Notation representations."@en;
    pov:registeredRepresentation ali:json-representation.

ali:html-representation-repository a pov:RepresentationRepository;
    rdfs:label "html representation repository"@en;
    rdfs:comment "Representation repository for HTML representations."@en;
    pov:registeredRepresentation ali:html-table-representation;
    pov:registeredRepresentation ali:html-inline-representation;
    pov:registeredRepresentation ali:html-link-representation;
    pov:registeredRepresentation ali:html-titled-list-representation.

ali:xul-representation-repository a pov:RepresentationRepository;
    rdfs:label "xul representation repository"@en;
    rdfs:comment "Representation repository for XUL representations."@en;
    pov:registeredRepresentation ali:xul-table-representation;
    pov:registeredRepresentation ali:xul-inline-representation;
    pov:registeredRepresentation ali:xul-link-representation;
    pov:registeredRepresentation ali:xul-titled-list-representation.

# -----------------------------------------------
# Individual Representation
# -----------------------------------------------

ali:text-table-representation a pov:Representation;
    rdfs:label "text table representation"@en;
    rdfs:comment "Plain text table represenation."@en;
    pov:intention ali:present;
    pov:intention ali:general;
    pov:intention ali:detail;
    pov:intention ali:related;
    pov:conformsTo ali:table;
    pov:decoration [a pov:TemplateDecoration;
    	pov:before "<% Iterator iter = displays.iterator(); while (iter.hasNext()) { %>${iter.next().rdfsLabel}<% if (iter.hasNext()) { %>\t<% } } %>\n";
    	pov:separation "\n"];
    pov:displayDecoration [a pov:TextDecoration;
    	pov:separation "\t"];
    pov:perspectiveDecoration ali:none;
    pov:searchDecoration ali:none;
    pov:literalDecoration [a pov:TextDecoration;
    	pov:separation " "].

ali:text-inline-representation a pov:Representation;
    rdfs:label "text inline representation"@en;
    rdfs:comment "Plain text table represenation."@en;
    pov:intention ali:reference;
    pov:intention ali:present;
    pov:intention ali:general;
    pov:intention ali:detail;
    pov:intention ali:related;
    pov:conformsTo ali:inline;
    pov:decoration [a pov:TextDecoration;
    	pov:separation "\n"];
    pov:displayDecoration [a pov:TextDecoration;
    	pov:separation " "];
    pov:perspectiveDecoration ali:none;
    pov:searchDecoration ali:none;
    pov:literalDecoration [a pov:TextDecoration;
    	pov:separation " "].

ali:json-representation a pov:Representation;
    rdfs:label "json array representation"@en;
    rdfs:comment "JavaScript Object Notation represenation in array format."@en;
    pov:intention ali:general;
    pov:intention ali:detail;
    pov:intention ali:related;
    pov:intention ali:reference;
    pov:conformsTo ali:table;
    pov:conformsTo ali:titled-list;
    pov:conformsTo ali:inline;
    pov:decoration [a pov:TextDecoration;
    	pov:before "[";
    	pov:separation ",";
    	pov:after "]";
    	pov:empty "[]"];
    pov:displayDecoration [a pov:TextDecoration;
    	pov:before "{";
    	pov:separation ",";
    	pov:after "}";
    	pov:empty "{}"];
    pov:perspectiveDecoration [a pov:TemplateDecoration;
    	pov:before "'<%=display.povName%>':";
    	pov:empty "'<%=display.povName%>':null"];
    pov:searchDecoration [a pov:TemplateDecoration;
    	pov:before "'<%=display.povName%>':[";
    	pov:separation ",";
    	pov:after "]";
    	pov:empty "'<%=display.povName%>':null"];
    pov:literalDecoration [a pov:TemplateDecoration;
    	pov:before "'<%=display.povName%>':['";
    	pov:separation "','";
    	pov:after "']";
    	pov:empty "'<%=display.povName%>':null"].

ali:html-inline-representation a pov:Representation;
    rdfs:label "html inline representation"@en;
    rdfs:comment "Plain HTML span represenation."@en;
    pov:intention ali:present;
    pov:intention ali:general;
    pov:intention ali:detail;
    pov:intention ali:related;
    pov:conformsTo ali:inline;
    pov:decoration [a pov:TextDecoration;
    	pov:before "\n<div class='representation'>\n";
    	pov:separation "\n";
		pov:after "\n</div>"];
    pov:displayDecoration [a pov:TextDecoration;
    	pov:before "<div class='element'>";
    	pov:separation "\n";
		pov:after "</div>"];
    pov:perspectiveDecoration [a pov:TextDecoration;
    	pov:before "<span class='display'><span class='child-representation'>";
		pov:after "</span></span>";
		pov:empty "<span class='display empty'/>"];
    pov:searchDecoration [a pov:TextDecoration;
    	pov:before "<span class='display'><span class='child-representation'>";
    	pov:separation "</span><span class='child-representation'>";
		pov:after "</span></span>";
		pov:empty "<span class='display empty'/>"];
    pov:literalDecoration [a pov:TextDecoration;
    	pov:before "<span class='display'><span class='value'>";
    	pov:separation "</span><span class='value'>";
		pov:after "</span></span>";
		pov:empty "<span class='display empty'/>"].

ali:html-link-representation a pov:Representation;
    rdfs:label "html link representation"@en;
    rdfs:comment "HTML link reference."@en;
    pov:intention ali:reference;
    pov:conformsTo ali:inline;
    pov:decoration [a pov:TemplateDecoration;
    	pov:separation "\n"];
    pov:displayDecoration [a pov:TemplateDecoration;
    	pov:before """<a class='representation element' <% if (resource.qName != null && resource.qName.prefix.length() > 0) { 
    		%> href='rdf:${resource.qName.namespaceURI}${resource.qName.localPart}' <% } %> >""";
    	pov:separation "\n";
    	pov:after "</a>"];
    pov:perspectiveDecoration [a pov:TextDecoration;
    	pov:before "<span class='display'><span class='child-representation'>";
		pov:after "</span></span>";
		pov:empty "?"];
    pov:searchDecoration [a pov:TextDecoration;
    	pov:before "<span class='display'><span class='child-representation'>";
    	pov:separation "</span><span class='child-representation'>";
		pov:after "</span></span>";
		pov:empty "?"];
    pov:literalDecoration [a pov:TextDecoration;
    	pov:before "<span class='display'><span class='value'>";
    	pov:separation "</span><span class='value'>";
		pov:after "</span></span>";
		pov:empty "?"].

ali:html-table-representation a pov:Representation;
    rdfs:label "html table representation"@en;
    rdfs:comment "Plain HTML table represenation."@en;
    pov:intention ali:present;
    pov:intention ali:general;
    pov:intention ali:detail;
    pov:intention ali:related;
    pov:conformsTo ali:table;
    pov:decoration [a pov:TemplateDecoration;
    	pov:before """\n<table class='representation'>
    	<thead>
    	<tr>
    	<% for (display in displays) { %>
    		<th>${display.rdfsLabel}</th>
    	<% } %>
    	</tr>
    	</thead>
    	<tbody>\n""";
    	pov:separation "\n";
		pov:after "\n</tbody></table>";
		pov:empty "<div class='representation empty'/>"];
    pov:displayDecoration [a pov:TextDecoration;
    	pov:before "<tr class='element'>";
    	pov:separation "\n";
		pov:after "</tr>"];
    pov:perspectiveDecoration [a pov:TextDecoration;
    	pov:before "<td class='display'><span class='child-representation'>";
		pov:after "</span></td>";
		pov:empty "<td class='display empty'> </td>"];
    pov:searchDecoration [a pov:TextDecoration;
    	pov:before "<td class='display'><span class='child-representation'>";
    	pov:separation "</span><span class='child-representation'>";
		pov:after "</span></td>";
		pov:empty "<td class='display empty'> </td>"];
    pov:literalDecoration [a pov:TextDecoration;
    	pov:before "<td class='display'><span class='value'>";
    	pov:separation "</span><span class='value'>";
		pov:after "</span></td>";
		pov:empty "<td class='display empty'> </td>"].

ali:html-titled-list-representation a pov:Representation;
    rdfs:label "html titled list representation"@en;
    rdfs:comment "List values with display titles."@en;
    pov:intention ali:present;
    pov:intention ali:general;
    pov:intention ali:detail;
    pov:intention ali:related;
    pov:conformsTo ali:titled-list;
    pov:decoration [a pov:TextDecoration;
    	pov:before "<div class='representation'>\n";
    	pov:separation "\n";
		pov:after "\n</div>";
		pov:empty "\n<div class='representation empty' />"];
    pov:displayDecoration [a pov:TextDecoration;
    	pov:before "<ul class='element'>";
    	pov:separation "\n";
		pov:after "</ul>"];
    pov:perspectiveDecoration [a pov:TemplateDecoration;
    	pov:before "<li class='display'><div class='label'>${display.rdfsLabel}</div><div class='child-representation'>";
    	pov:after "</div></li>"];
    pov:searchDecoration [a pov:TemplateDecoration;
    	pov:before "<li class='display'><div class='label'>${display.rdfsLabel}</div><div class='child-representation'>";
    	pov:separation "</div><div class='child-representation'>";
    	pov:after "</div></li>"];
    pov:literalDecoration [a pov:TemplateDecoration;
    	pov:before "<li class='display'><div class='label'>${display.rdfsLabel}</div><div class='value'>";
    	pov:separation "</div><div class='value'>";
		pov:after "</div></li>"].

ali:xul-inline-representation a pov:Representation;
    rdfs:label "xul inline representation"@en;
    rdfs:comment "Plain XUL span represenation."@en;
    pov:intention ali:present;
    pov:intention ali:general;
    pov:intention ali:detail;
    pov:intention ali:related;
    pov:conformsTo ali:inline;
    pov:decoration [a pov:TextDecoration;
    	pov:before "\n<box class='representation'>";
    	pov:separation "\n";
		pov:after "\n</box>"];
    pov:displayDecoration [a pov:TextDecoration;
    	pov:before "<box class='element'>";
    	pov:separation "\n";
		pov:after "</box>"];
    pov:perspectiveDecoration [a pov:TextDecoration;
    	pov:before "<box class='display'><box class='child-representation'>";
		pov:after "</box></box>";
		pov:empty "<box class='display empty'/>"];
    pov:searchDecoration [a pov:TextDecoration;
    	pov:before "<box class='display'><box class='child-representation'>";
    	pov:separation "</box><box class='child-representation'>";
		pov:after "</box></box>";
		pov:empty "<box class='display empty'/>"];
    pov:literalDecoration [a pov:TextDecoration;
    	pov:before "<box class='display'><label class='value'>";
    	pov:separation "</label><label class='value'>";
		pov:after "</label></box>";
		pov:empty "<box class='display empty'/>"].

ali:xul-link-representation a pov:Representation;
    rdfs:label "xul link representation"@en;
    rdfs:comment "XUL link reference."@en;
    pov:intention ali:reference;
    pov:conformsTo ali:inline;
    pov:decoration ali:none;
    pov:displayDecoration [a pov:TextDecoration;
    	pov:before "\n<box>";
    	pov:separation "\n";
    	pov:after "\n</box>"];
    pov:perspectiveDecoration [a pov:TextDecoration;
    	pov:before "<box class='display'><box class='child-representation'>";
		pov:after "</box></box>";
		pov:empty "?"];
    pov:searchDecoration [a pov:TextDecoration;
    	pov:before "<box class='display'><box class='child-representation'>";
    	pov:separation "</box><box class='child-representation'>";
		pov:after "</box></box>";
		pov:empty "?"];
    pov:literalDecoration [a pov:TemplateDecoration;
    	pov:before """<box class='display'><label <% if (resource.qName != null && resource.qName.prefix.length() > 0) {
    		%> class='text-link representation element' onclick="document.location.href='rdf:${resource.qName.namespaceURI}${resource.qName.localPart}'" <%
    		} else { %> class='representation element' <% }%> >""";
    	pov:separation """</label><label <% if (resource.qName != null && resource.qName.prefix.length() > 0) {
    		%> class='text-link value' onclick="document.location.href='rdf:${resource.qName.namespaceURI}${resource.qName.localPart}'" <%
    		} else { %> class='value' <% }%> >""";
		pov:after "</label></box>";
		pov:empty "?"].

ali:xul-table-representation a pov:Representation;
    rdfs:label "xul table representation"@en;
    rdfs:comment "Plain XUL table represenation."@en;
    pov:intention ali:present;
    pov:intention ali:general;
    pov:intention ali:detail;
    pov:intention ali:related;
    pov:conformsTo ali:table;
    pov:decoration [a pov:TemplateDecoration;
    	pov:before """\n<grid class='representation'>
    	<rows>
    	<row>
    	<% for (display in displays) { %>
    		<label>${display.rdfsLabel}</label>
    	<% } %>
    	</row>""";
    	pov:separation "\n";
		pov:after "\n</rows></grid>";
		pov:empty "<grid class='representation empty'/>"];
    pov:displayDecoration [a pov:TextDecoration;
    	pov:before "<row class='element'>";
    	pov:separation "\n";
		pov:after "</row>"];
    pov:perspectiveDecoration [a pov:TextDecoration;
    	pov:before "<vbox class='display'><box class='child-representation'>";
		pov:after "</box></vbox>";
		pov:empty "<vbox class='display empty'/>"];
    pov:searchDecoration [a pov:TextDecoration;
    	pov:before "<vbox class='display'><box class='child-representation'>";
    	pov:separation "</box><box class='child-representation'>";
		pov:after "</box></vbox>";
		pov:empty "<vbox class='display empty'/>"];
    pov:literalDecoration [a pov:TextDecoration;
    	pov:before "<vbox class='display'><label class='value'>";
    	pov:separation "</label><label class='value'>";
		pov:after "</label></vbox>";
		pov:empty "<vbox class='display empty'/>"].

ali:xul-titled-list-representation a pov:Representation;
    rdfs:label "xul titled list representation"@en;
    rdfs:comment "List values with display titles."@en;
    pov:intention ali:present;
    pov:intention ali:general;
    pov:intention ali:detail;
    pov:intention ali:related;
    pov:conformsTo ali:titled-list;
    pov:decoration [a pov:TextDecoration;
    	pov:before "\n<vbox class='representation'>";
    	pov:separation "\n";
		pov:after "\n</vbox>";
		pov:empty "\n<vbox class='representation empty' />"];
    pov:displayDecoration [a pov:TemplateDecoration;
    	pov:before "<groupbox class='element'><caption><label style='text-transform:capitalize'><%=resource.rdfsLabel%></label></caption>";
    	pov:separation "\n";
		pov:after "</groupbox>"];
    pov:perspectiveDecoration [a pov:TemplateDecoration;
    	pov:before "<box><groupbox class='display child-representation'><caption><label class='header' style='text-transform:capitalize'>${display.rdfsLabel}</label></caption>";
    	pov:after "</groupbox><box flex='1'/></box>"];
    pov:searchDecoration [a pov:TemplateDecoration;
    	pov:before "<groupbox class='display child-representation'><caption><label class='header' style='text-transform:capitalize'>${display.rdfsLabel}</label></caption>";
    	pov:after "</groupbox>"];
    pov:literalDecoration [a pov:TemplateDecoration;
    	pov:before "<vbox class='display'><label class='header' style='text-transform:capitalize'>${display.rdfsLabel}</label><label class='indent value'>";
    	pov:separation "</label><label class='indent value'>";
		pov:after "</label></vbox>"].

# -----------------------------------------------
# Individual PerspectiveRepositories
# -----------------------------------------------

ali:perspective-repository a pov:PerspectiveRepository;
    rdfs:label "perspective repository"@en;
    rdfs:comment "The main perspective repository."@en.

# -----------------------------------------------
# Individual PerspectiveFactories
# -----------------------------------------------

ali:perspective-factory a pov:PerspectiveFactory;
    rdfs:label "perspective factory"@en;
    rdfs:comment "The main perspective factory."@en;
    pov:displayFactory ali:display-factory.

# -----------------------------------------------
# Individual Perspectives
# -----------------------------------------------

ali:reference a pov:Perspective;
	pov:purpose ali:reference;
	pov:layout ali:inline;
	pov:displays [a rdf:Seq;
		rdf:_1 [a pov:LiteralDisplay;
			pov:style ali:normal;
			pov:format ali:qualified-name]].

# -----------------------------------------------
# Individual Intents
# -----------------------------------------------

ali:reference a pov:Intent;
    rdfs:label "reference"@en;
    rdfs:comment "Intended to display information to uniquely identify resource."@en.

ali:present a pov:Intent;
    rdfs:label "present"@en;
    rdfs:comment "Intended for resource presentations with basic information."@en;
    pov:aim ali:reference.

ali:general a pov:Intent;
    rdfs:label "general"@en;
    rdfs:comment "Intended as a general use for on screen display."@en;
    pov:aim ali:present.

ali:detail a pov:Intent;
    rdfs:label "detail"@en;
    rdfs:comment "Intended to display more detailed information."@en;
    pov:aim ali:general.

ali:related a pov:Intent;
    rdfs:label "related"@en;
    rdfs:comment "Intended to display related information."@en;
    pov:aim ali:general.

# -----------------------------------------------
# Individual Layouts
# -----------------------------------------------

ali:value-list a pov:Layout;
    rdfs:label "value list"@en;
    rdfs:comment "Display the values vertically without titles.  The heading and footing are displayed above and below the list without their display titles."@en.

ali:titled-list a pov:Layout;
    rdfs:label "titled list"@en;
    rdfs:comment "Display indented values vertically with display title above.  The heading and footing are displayed above and below the list without their display titles."@en.

ali:indented-list a pov:Layout;
    rdfs:label "indented list"@en;
    rdfs:comment "Display indented values vertically with display title above.  The heading and footing are displayed to the left of the list without their display titles."@en.

ali:aligned-list a pov:Layout;
    rdfs:label "aligned list"@en;
    rdfs:comment "Display the values vertically with titles and values vertically aligned."@en.

ali:inline a pov:Layout;
    rdfs:label "inline"@en;
    rdfs:comment "Display the values horizontally without titles.  Heading and footing appeare above and below the body without their display titles."@en.

ali:table a pov:Layout;
    rdfs:label "table"@en;
    rdfs:comment "Display the values in columns with title headers.  Heading and footing are presented at the top and bottom of the table everywhere it appears without their display titles."@en.

ali:tabbed a pov:Layout;
    rdfs:label "tabbed"@en;
    rdfs:comment "Only one body display is visible at a time, using the display title as the label.  Heading and footing are presented at the top and bottom of the view everywhere it appears without their display titles."@en.

