# -----------------------------------------------
# RepresentationRepository Properties
# -----------------------------------------------

pov:registeredRepresentation a owl:ObjectProperty;
    rdfs:label "registered representation"@en;
    rdfs:comment "Set of active representation in repository."@en;
    rdfs:isDefinedBy pov:;
    rdfs:domain pov:RepresentationRepository;
    rdfs:range pov:Representation.

# -----------------------------------------------
# Representation Properties
# -----------------------------------------------

# pov:intention

# pov:decoration

pov:conformsTo a owl:ObjectProperty;
    rdfs:label "conforms to"@en;
    rdfs:comment "The layouts that are supported by this Representation."@en;
    rdfs:isDefinedBy pov:;
    rdfs:domain pov:Representation;
    rdfs:range pov:Layout.

pov:displayDecoration a owl:ObjectProperty; a owl:FunctionalProperty;
    rdfs:label "display decoration"@en;
    rdfs:comment "Decoration around the displays."@en;
    rdfs:isDefinedBy pov:;
    rdfs:domain pov:Representation;
    rdfs:range pov:Decoration.

pov:perspectiveDecoration a owl:ObjectProperty; a owl:FunctionalProperty;
    rdfs:label "perspective decoration"@en;
    rdfs:comment "Decoration around perspective display values."@en;
    rdfs:isDefinedBy pov:;
    rdfs:domain pov:Representation;
    rdfs:range pov:Decoration.

pov:searchDecoration a owl:ObjectProperty; a owl:FunctionalProperty;
    rdfs:label "search decoration"@en;
    rdfs:comment "Decoration around search display values."@en;
    rdfs:isDefinedBy pov:;
    rdfs:domain pov:Representation;
    rdfs:range pov:Decoration.

pov:literalDecoration a owl:ObjectProperty; a owl:FunctionalProperty;
    rdfs:label "literal decoration"@en;
    rdfs:comment "Decoration around literal display values."@en;
    rdfs:isDefinedBy pov:;
    rdfs:domain pov:Representation;
    rdfs:range pov:Decoration.

# -----------------------------------------------
# PerspectiveRepository Properties
# -----------------------------------------------

pov:registeredPerspective a owl:ObjectProperty;
    rdfs:label "registered perspective"@en;
    rdfs:comment "Set of active perspectives in repository."@en;
    rdfs:isDefinedBy pov:;
    rdfs:domain pov:PerspectiveRepository;
    rdfs:range pov:Perspective.

# -----------------------------------------------
# PerspectiveFactory Properties
# -----------------------------------------------

pov:displayFactory a owl:ObjectProperty; a owl:FunctionalProperty;
    rdfs:label "display factory"@en;
    rdfs:comment "Factory used to create displays."@en;
    rdfs:isDefinedBy pov:;
    rdfs:domain pov:PerspectiveFactory;
    rdfs:range pov:DisplayFactory.

# -----------------------------------------------
# Perspective Properties
# -----------------------------------------------

pov:purpose a owl:ObjectProperty; a owl:FunctionalProperty;
    rdfs:label "purpose"@en;
    rdfs:comment "A representation must be selected based on this purpose."@en;
    rdfs:isDefinedBy pov:;
    rdfs:domain [a owl:Class; owl:unionOf (pov:Perspective pov:SearchPattern)];
    rdfs:range pov:Intent.

pov:layout a owl:ObjectProperty; a owl:FunctionalProperty;
    rdfs:label "layout"@en;
    rdfs:comment "The layout that is or should be used."@en;
    rdfs:isDefinedBy pov:;
    rdfs:domain [a owl:Class; owl:unionOf (pov:Perspective pov:SearchPattern)];
    rdfs:range pov:Layout.

pov:represents a owl:ObjectProperty;
    rdfs:label "represents"@en;
    rdfs:comment "Specifies what type of indiviuals this perspective can be used for."@en;
    rdfs:isDefinedBy pov:;
    rdfs:domain [a owl:Class; owl:unionOf (pov:Perspective pov:SearchPattern)];
    rdfs:range rdfs:Class.

pov:displays a owl:ObjectProperty; a owl:FunctionalProperty;
    rdfs:label "displays"@en;
    rdfs:comment "List of all Displays which should be shown in this perspective."@en;
    rdfs:isDefinedBy pov:;
    rdfs:domain [a owl:Class; owl:unionOf (pov:Perspective pov:SearchPattern)];
    rdfs:range rdf:Seq.

# -----------------------------------------------
# SearchPatternRepository Properties
# -----------------------------------------------

pov:registeredSearchPattern a owl:ObjectProperty;
    rdfs:label "registered search pattern"@en;
    rdfs:comment "Set of active search patterns in repository."@en;
    rdfs:isDefinedBy pov:;
    rdfs:domain pov:SearchPatternRepository;
    rdfs:range pov:SearchPattern.

# -----------------------------------------------
# SearchPattern Properties
# -----------------------------------------------

# pov:purpose

# pov:layout

# pov:represents

# pov:displays

pov:selectExpression a owl:ObjectProperty; a owl:FunctionalProperty;
    rdfs:label "select expression"@en;
    rdfs:comment "The primary query expression, including SELECT, FROM, and begin WHERE."@en;
    rdfs:isDefinedBy pov:;
    rdfs:domain pov:SearchPattern;
    rdfs:range pov:Expression.

pov:filterExpressions a owl:ObjectProperty; a owl:FunctionalProperty;
    rdfs:label "query"@en;
    rdfs:comment "The FILTER or WHERE expression(s)."@en;
    rdfs:isDefinedBy pov:;
    rdfs:domain pov:SearchPattern;
    rdfs:range pov:ExpressionRepository.

pov:groupByExpression a owl:ObjectProperty; a owl:FunctionalProperty;
    rdfs:label "group by expression"@en;
    rdfs:comment "The query expression after the filters, including closing WHERE and GROUP BY."@en;
    rdfs:isDefinedBy pov:;
    rdfs:domain pov:SearchPattern;
    rdfs:range pov:Expression.

pov:orderByExpressions a owl:ObjectProperty; a owl:FunctionalProperty;
    rdfs:label "order by expressions"@en;
    rdfs:comment "The ORDER BY expressions for a search pattern."@en;
    rdfs:isDefinedBy pov:;
    rdfs:domain pov:SearchPattern;
    rdfs:range pov:OrderByRepository.

# -----------------------------------------------
# ExpressionRepository Properties
# -----------------------------------------------

pov:defaultExpression a owl:ObjectProperty; a owl:FunctionalProperty;
    rdfs:label "default expression"@en;
    rdfs:comment "A default expression."@en;
    rdfs:isDefinedBy pov:;
    rdfs:domain pov:ExpressionRepository;
    rdfs:range pov:Expression.

pov:registeredExpression a owl:ObjectProperty;
    rdfs:label "registered expression"@en;
    rdfs:comment "Set of expressions."@en;
    rdfs:isDefinedBy pov:;
    rdfs:domain pov:ExpressionRepository;
    rdfs:range pov:Expression.

# -----------------------------------------------
# Expression Properties
# -----------------------------------------------

pov:name a owl:DatatypeProperty; a owl:FunctionalProperty;
    rdfs:label "name"@en;
    rdfs:comment "Binding name used within query."@en;
    rdfs:isDefinedBy pov:;
    rdfs:domain [a owl:Class; owl:unionOf (pov:Expression pov:Display)];
    rdfs:range xsd:string.

pov:binding a owl:ObjectProperty;
    rdfs:label "binding"@en;
    rdfs:comment "Binding display parameters to be used when evaluating this expression."@en;
    rdfs:isDefinedBy pov:;
    rdfs:domain pov:Expression;
    rdfs:range pov:LiteralDisplay.

pov:inSparql a owl:DatatypeProperty; a owl:FunctionalProperty;
    rdfs:label "in SPARQL"@en;
    rdfs:comment "The query represented in SPARQL RDF Query Language."@en;
    rdfs:isDefinedBy pov:;
    rdfs:domain pov:Expression;
    rdfs:range xsd:string.

pov:inSerql a owl:DatatypeProperty; a owl:FunctionalProperty;
    rdfs:label "in SeRQL"@en;
    rdfs:comment "The query represented in Sesame RDF Query Language."@en;
    rdfs:isDefinedBy pov:;
    rdfs:domain pov:Expression;
    rdfs:range xsd:string.

pov:inJpql a owl:DatatypeProperty; a owl:FunctionalProperty;
    rdfs:label "in JPQL"@en;
    rdfs:comment "The query represented in Java Persistence Query Language."@en;
    rdfs:isDefinedBy pov:;
    rdfs:domain pov:Expression;
    rdfs:range xsd:string.

pov:inSql a owl:DatatypeProperty; a owl:FunctionalProperty;
    rdfs:label "in SQL"@en;
    rdfs:comment "The query represented in Structured Query Language."@en;
    rdfs:isDefinedBy pov:;
    rdfs:domain pov:Expression;
    rdfs:range xsd:string.

