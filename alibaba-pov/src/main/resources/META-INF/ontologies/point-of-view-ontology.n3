@prefix rdf:        <http://www.w3.org/1999/02/22-rdf-syntax-ns#>.
@prefix rdfs:       <http://www.w3.org/2000/01/rdf-schema#>.
@prefix dc:         <http://purl.org/dc/elements/1.1/>.
@prefix xsd:        <http://www.w3.org/2001/XMLSchema#>.
@prefix owl:        <http://www.w3.org/2002/07/owl#>.
@prefix pov:        <http://www.openrdf.org/rdf/2007/08/point-of-view#>.

# -----------------------------------------------
# Ontology
# -----------------------------------------------

pov: a owl:Ontology;
    rdfs:label       "Point of View Ontology"@en;
    rdfs:comment     "Ontology for defining display perspectives of resources."@en;
    dc:date          "2007-05-04"^^xsd:date;
    dc:format        "OWL-DL"@en;
    dc:creator       "James Leigh <james@leighnet.ca>";
    owl:imports      <http://www.w3.org/2002/07/owl#>;
    owl:imports      <http://www.w3.org/2000/01/rdf-schema#>;
    owl:imports      <http://purl.org/dc/elements/1.1/>;
    owl:imports      <http://www.w3.org/2001/XMLSchema#>;
    owl:versionInfo  "0.1".

dc:creator a owl:AnnotationProperty.

# -----------------------------------------------
# Classes
# -----------------------------------------------

pov:PresentationRepository a owl:Class;
    rdfs:label "Presentation Repository"@en;
    rdfs:comment "Aggregate presentations."@en;
    rdfs:isDefinedBy pov:;
    rdfs:subClassOf owl:Thing.

pov:Presentation a owl:Class;
    rdfs:label "Presentation"@en;
    rdfs:comment "Presentation of representations."@en;
    rdfs:isDefinedBy pov:;
    rdfs:subClassOf owl:Thing.

pov:TextPresentation a owl:Class;
    rdfs:label "Text Presentation"@en;
    rdfs:comment "Text based presentation of representations."@en;
    rdfs:isDefinedBy pov:;
    rdfs:subClassOf pov:Presentation.

pov:Decoration a owl:Class;
    rdfs:label "Decoration"@en;
    rdfs:comment "Decortation used by presentations and representations."@en;
    rdfs:isDefinedBy pov:;
    rdfs:subClassOf owl:Thing.

pov:TextDecoration a owl:Class;
    rdfs:label "Text Decoration"@en;
    rdfs:comment "Decortation of static text used by presentations and representations."@en;
    rdfs:isDefinedBy pov:;
    rdfs:subClassOf pov:Decoration.

pov:TemplateDecoration a owl:Class;
    rdfs:label "Template Decoration"@en;
    rdfs:comment "Decortation of Groovy Simple Templates used by presentations and representations."@en;
    rdfs:isDefinedBy pov:;
    rdfs:subClassOf pov:TextDecoration.

pov:Encoding a owl:Class;
    rdfs:label "Encoding"@en;
    rdfs:comment "Encoding used to insert values into a representation."@en;
    rdfs:isDefinedBy pov:;
    rdfs:subClassOf owl:Thing.

pov:RepresentationRepository a owl:Class;
    rdfs:label "Representation Repository"@en;
    rdfs:comment "Set of representations with a common content-type."@en;
    rdfs:isDefinedBy pov:;
    rdfs:subClassOf owl:Thing.

pov:Representation a owl:Class;
    rdfs:label "Reprenentation"@en;
    rdfs:comment "A format that can be used to represent resources."@en;
    rdfs:isDefinedBy pov:;
    rdfs:subClassOf owl:Thing.

pov:PerspectiveRepository a owl:Class;
    rdfs:label "Perspective Repository"@en;
    rdfs:comment "Aggregate repository of perspectives."@en;
    rdfs:isDefinedBy pov:;
    rdfs:subClassOf owl:Thing.

pov:PerspectiveFactory a owl:Class;
    rdfs:label "Perspective Factory"@en;
    rdfs:comment "Use to create perspectives as needed."@en;
    rdfs:isDefinedBy pov:;
    rdfs:subClassOf owl:Thing.

pov:Perspective a owl:Class;
    rdfs:label "Perspective"@en;
    rdfs:comment "Display perspective for a resource identified by one or more of it classes."@en;
    rdfs:isDefinedBy pov:;
    rdfs:subClassOf owl:Thing.

pov:SearchPatternRepository a owl:Class;
    rdfs:label "Search Pattern Repository"@en;
    rdfs:comment "Repository of search patterns"@en;
    rdfs:isDefinedBy pov:;
    rdfs:subClassOf owl:Thing.

pov:SearchPattern a owl:Class;
    rdfs:label "Search Pattern"@en;
    rdfs:comment "Describes a query and how to display it."@en;
    rdfs:isDefinedBy pov:;
    rdfs:subClassOf owl:Thing.

pov:ExpressionRepository a owl:Class;
    rdfs:label "Expression Repository"@en;
    rdfs:comment "Repository of Expressions."@en;
    rdfs:isDefinedBy pov:;
    rdfs:subClassOf owl:Thing.

pov:Expression a owl:Class;
    rdfs:label "Expression"@en;
    rdfs:comment "Describes part of a query."@en;
    rdfs:isDefinedBy pov:;
    rdfs:subClassOf owl:Thing.

pov:OrderByRepository a owl:Class;
    rdfs:label "ORDER BY Expression Repository"@en;
    rdfs:comment "Repository of ORDER BY Expressions."@en;
    rdfs:isDefinedBy pov:;
    rdfs:subClassOf pov:ExpressionRepository.

pov:OrderByExpression a owl:Class;
    rdfs:label "ORDER BY Expression"@en;
    rdfs:comment "Describes an ORDER BY expression for a display."@en;
    rdfs:isDefinedBy pov:;
    rdfs:subClassOf pov:Expression.

pov:Intent a owl:Class;
    rdfs:label "Intent"@en;
    rdfs:comment "The propose or intent in which a specific persective might be appropriate."@en;
    rdfs:isDefinedBy pov:;
    rdfs:subClassOf owl:Thing.

pov:Layout a owl:Class;
    rdfs:label "Layout"@en;
    rdfs:comment "Layout used for display values."@en;
    rdfs:isDefinedBy pov:;
    rdfs:subClassOf owl:Thing.

pov:DisplayRepository a owl:Class;
    rdfs:label "Display Repository"@en;
    rdfs:comment "Repository of active displays."@en;
    rdfs:isDefinedBy pov:;
    rdfs:subClassOf owl:Thing.

pov:DisplayFactory a owl:Class;
    rdfs:label "Display Factory"@en;
    rdfs:comment "Used to create displays as needed."@en;
    rdfs:isDefinedBy pov:;
    rdfs:subClassOf owl:Thing.

pov:Display a owl:Class;
    rdfs:label "Display"@en;
    rdfs:comment "Description of how a value should be shown."@en;
    rdfs:isDefinedBy pov:;
    rdfs:subClassOf owl:Thing.

pov:LiteralDisplay a owl:Class;
    rdfs:label "Format Display"@en;
    rdfs:comment "Description of how resources should be formatted and shown."@en;
    rdfs:isDefinedBy pov:;
    rdfs:subClassOf pov:Display.

pov:PerspectiveDisplay a owl:Class;
    rdfs:label "Perspective Display"@en;
    rdfs:comment "Description of how resources should be shown."@en;
    rdfs:isDefinedBy pov:;
    rdfs:subClassOf pov:Display.

pov:SearchDisplay a owl:Class;
    rdfs:label "Search Display"@en;
    rdfs:comment "Description of how resources that should be retrived and shown."@en;
    rdfs:isDefinedBy pov:;
    rdfs:subClassOf pov:Display.

pov:Size a owl:Class;
    rdfs:label "Size"@en;
    rdfs:comment "Contains the width and height restrictions of a display."@en;
    rdfs:isDefinedBy pov:;
    rdfs:subClassOf owl:Thing.

pov:Length a owl:Class;
    rdfs:label "Length"@en;
    rdfs:comment "Measurement of a width or height for a display."@en;
    rdfs:isDefinedBy pov:;
    rdfs:subClassOf owl:Thing.

pov:Style a owl:Class;
    rdfs:label "Style"@en;
    rdfs:comment "Display style for formatted values."@en;
    rdfs:isDefinedBy pov:;
    rdfs:subClassOf owl:Thing.

pov:FormatRepository a owl:Class;
    rdfs:label "Format Repository"@en;
    rdfs:comment "Repository of active formats."@en;
    rdfs:isDefinedBy pov:;
    rdfs:subClassOf owl:Thing.

pov:Format a owl:Class;
    rdfs:label "Format"@en;
    rdfs:comment "Describes how a resource should appear."@en;
    rdfs:isDefinedBy pov:;
    rdfs:subClassOf owl:Thing.

pov:MessagePatternFormat a owl:Class;
    rdfs:label "Message Pattern Format"@en;
    rdfs:comment "Indicates how property values should be formatted."@en;
    rdfs:isDefinedBy pov:;
    rdfs:subClassOf pov:Format.

# -----------------------------------------------
# Properties
# -----------------------------------------------

# -----------------------------------------------
# PresentationRepository Properties
# -----------------------------------------------

pov:registeredPresentation a owl:ObjectProperty;
    rdfs:label "registered presentation"@en;
    rdfs:comment "Set of active presentations in repository."@en;
    rdfs:isDefinedBy pov:;
    rdfs:domain pov:PresentationRepository;
    rdfs:range pov:Presentation.

# -----------------------------------------------
# Presentation Properties
# -----------------------------------------------

pov:accept a owl:DatatypeProperty;
    rdfs:label "accept"@en;
    rdfs:comment "Set of accept strings that can be used with this presentation."@en;
    rdfs:isDefinedBy pov:;
    rdfs:domain pov:Presentation;
    rdfs:range xsd:string.

pov:contentType a owl:DatatypeProperty; a owl:FunctionalProperty;
    rdfs:label "content type"@en;
    rdfs:comment "The content-type this presentation generates."@en;
    rdfs:isDefinedBy pov:;
    rdfs:domain pov:Presentation;
    rdfs:range xsd:string.

pov:intention a owl:ObjectProperty;
    rdfs:label "intention"@en;
    rdfs:comment "This intention is matched against the purpose."@en;
    rdfs:isDefinedBy pov:;
    rdfs:domain [a owl:Class; owl:unionOf (pov:Presentation pov:Representation)];
    rdfs:range pov:Intent.

pov:encoding a owl:DatatypeProperty; a owl:FunctionalProperty;
    rdfs:label "encoding"@en;
    rdfs:comment "Method used to serialize values."@en;
    rdfs:isDefinedBy pov:;
    rdfs:domain pov:Presentation;
    rdfs:range pov:Encoding.

pov:representations a owl:ObjectProperty; a owl:FunctionalProperty;
    rdfs:label "representations"@en;
    rdfs:comment "The representation repository used to lookup representations."@en;
    rdfs:isDefinedBy pov:;
    rdfs:domain pov:Presentation;
    rdfs:range pov:RepresentationRepository.

pov:searchPatterns a owl:ObjectProperty; a owl:FunctionalProperty;
    rdfs:label "search patterns"@en;
    rdfs:comment "The search pattern repository used to lookup search patterns."@en;
    rdfs:isDefinedBy pov:;
    rdfs:domain pov:Presentation;
    rdfs:range pov:SearchPatternRepository.

pov:perspectives a owl:ObjectProperty; a owl:FunctionalProperty;
    rdfs:label "perspectives"@en;
    rdfs:comment "The perspective repository used to lookup perspectives."@en;
    rdfs:isDefinedBy pov:;
    rdfs:domain pov:Presentation;
    rdfs:range pov:PerspectiveRepository.

pov:formats a owl:ObjectProperty; a owl:FunctionalProperty;
    rdfs:label "formats"@en;
    rdfs:comment "The format repository used to lookup formats."@en;
    rdfs:isDefinedBy pov:;
    rdfs:domain pov:Presentation;
    rdfs:range pov:FormatRepository.

pov:perspectiveFactory a owl:ObjectProperty; a owl:FunctionalProperty;
    rdfs:label "perspective factory"@en;
    rdfs:comment "The perspective factory used to create missing perspectives."@en;
    rdfs:isDefinedBy pov:;
    rdfs:domain pov:Presentation;
    rdfs:range pov:PerspectiveFactory.

pov:decoration a owl:ObjectProperty; a owl:FunctionalProperty;
    rdfs:label "decoration"@en;
    rdfs:comment "The decoration used when rendering the presentation or representation"@en;
    rdfs:isDefinedBy pov:;
    rdfs:domain [a owl:Class; owl:unionOf (pov:Presentation pov:Representation)];
    rdfs:range pov:Decoration.

