# -----------------------------------------------
# TextDecoration Properties
# -----------------------------------------------

pov:before a owl:DatatypeProperty; a owl:FunctionalProperty;
    rdfs:label "before"@en;
    rdfs:comment "Text to include before the value."@en;
    rdfs:isDefinedBy pov:;
    rdfs:domain pov:TextDecoration;
    rdfs:range xsd:string.

pov:separation a owl:DatatypeProperty; a owl:FunctionalProperty;
    rdfs:label "separation"@en;
    rdfs:comment "Text to include between values."@en;
    rdfs:isDefinedBy pov:;
    rdfs:domain pov:TextDecoration;
    rdfs:range xsd:string.

pov:after a owl:DatatypeProperty; a owl:FunctionalProperty;
    rdfs:label "after"@en;
    rdfs:comment "Text to include after the value."@en;
    rdfs:isDefinedBy pov:;
    rdfs:domain pov:TextDecoration;
    rdfs:range xsd:string.

pov:empty a owl:DatatypeProperty; a owl:FunctionalProperty;
    rdfs:label "empty"@en;
    rdfs:comment "Text to use if no value exists."@en;
    rdfs:isDefinedBy pov:;
    rdfs:domain pov:TextDecoration;
    rdfs:range xsd:string.

