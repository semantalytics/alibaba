pov:ascending a owl:ObjectProperty;
    rdfs:label "asceding"@en;
    rdfs:comment "The display this expression is ordered by ascending"@en;
    rdfs:isDefinedBy pov:;
    rdfs:domain pov:OrderByExpression;
    rdfs:range pov:Display.

pov:descending a owl:ObjectProperty;
    rdfs:label "descending"@en;
    rdfs:comment "The display this expression is ordered by descending"@en;
