pov:style a owl:ObjectProperty; a owl:FunctionalProperty;
    rdfs:label "style"@en;
    rdfs:comment "The style that should be used for this display."@en;
    rdfs:isDefinedBy pov:;
    rdfs:domain pov:Display;
    rdfs:range pov:Style.

