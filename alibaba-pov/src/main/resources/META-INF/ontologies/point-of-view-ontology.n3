pov:format a owl:ObjectProperty; a owl:FunctionalProperty;
    rdfs:label "format"@en;
    rdfs:comment "The format that should be used for displaying the literal values."@en;
    rdfs:isDefinedBy pov:;
    rdfs:domain pov:LiteralDisplay;
    rdfs:range pov:Format.

