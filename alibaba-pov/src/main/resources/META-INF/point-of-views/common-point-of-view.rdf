        <displays rdf:parseType="Resource">
            <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Seq"/>
            <rdf:_1 rdf:parseType="Resource">
                <rdf:type rdf:resource="http://www.openrdf.org/rdf/2007/09/point-of-view#QNameDisplay"/>
                <format rdf:resource="http://www.openrdf.org/rdf/2007/09/alibaba/qualified-name"/>
