            </rdf:_1>
        </displays>
        <layout rdf:resource="http://www.openrdf.org/rdf/2007/09/alibaba/inline"/>
        <lookup rdf:parseType="Resource">
            <rdf:type rdf:resource="http://www.openrdf.org/rdf/2007/09/point-of-view#SearchPattern"/>
            <filterExpressions rdf:parseType="Resource">
                <rdf:type rdf:resource="http://www.openrdf.org/rdf/2007/09/point-of-view#ExpressionRepository"/>
                <registeredExpression rdf:parseType="Resource">
                    <rdf:type rdf:resource="http://www.openrdf.org/rdf/2007/09/point-of-view#Expression"/>
                    <inSparql>FILTER </inSparql>
                </registeredExpression>
            </filterExpressions>
            <selectExpression rdf:parseType="Resource">
                <rdf:type rdf:resource="http://www.openrdf.org/rdf/2007/09/point-of-view#Expression"/>
                <inSparql>SELECT ?resource WHERE {?resource }</inSparql>
            </selectExpression>
        </lookup>
        <purpose rdf:resource="http://www.openrdf.org/rdf/2007/09/alibaba/reference"/>
        <rdfs:comment xml:lang="en">Intended to display information to uniquely identify resource.</rdfs:comment>
        <rdfs:label xml:lang="en">reference</rdfs:label>
    </Intent>

