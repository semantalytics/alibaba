<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE web-app
      PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
      "http://java.sun.com/dtd/web-app_2_3.dtd">

<web-app>
  <display-name>alibaba</display-name>
  <servlet>
    <servlet-name>alibaba</servlet-name>
    <servlet-class>org.openrdf.alibaba.servlet.impl.AlibabaServlet</servlet-class>
    <init-param>
      <param-name>applicationId</param-name>
      <param-value>alibaba</param-value>
    </init-param>
    <init-param>
      <param-name>repositoryId</param-name>
      <param-value>default</param-value>
    </init-param>
    <init-param>
      <param-name>initData</param-name>
      <param-value>
      META-INF/point-of-views/common-point-of-view.rdf;
      META-INF/point-of-views/decor-point-of-view.rdf;
      META-INF/decors/common-decor.nt;
      META-INF/decors/html-decor.nt;
      META-INF/decors/json-decor.nt;
      META-INF/decors/text-decor.nt;
      META-INF/decors/xul-decor.nt;
      META-INF/decors/povml-decor.nt
      </param-value>
    </init-param>
    <load-on-startup>1</load-on-startup>
  </servlet>
  <servlet-mapping>
    <servlet-name>alibaba</servlet-name>
    <url-pattern>/alibaba/*</url-pattern>
  </servlet-mapping>
</web-app>
