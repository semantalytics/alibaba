<?xml version="1.0"  encoding="ISO-8859-1"?>
<!DOCTYPE Configure PUBLIC 
 "-//Mort Bay Consulting//DTD Configure 1.2//EN"
 "http://jetty.mortbay.org/configure_1_2.dtd">
<Configure class="org.mortbay.jetty.Server">
  <Call name="addConnector">
    <Arg>
      <New class="org.mortbay.jetty.bio.SocketConnector">
        <Set name="Port" type="int">6139</Set>
      </New>
    </Arg>
  </Call>
                                                                        
  <Call name="addHandler">
    <Arg>
      <New class="org.mortbay.jetty.servlet.Context">
        <Call name="addServlet">
          <Arg>
            <New class="org.mortbay.jetty.servlet.ServletHolder">
              <Set name="Servlet">
                <New class="org.openrdf.alibaba.servlet.impl.AlibabaServlet"/>
              </Set>
              <Set name="InitParameters">
                <New class="java.util.HashMap">
                  <Put name="applicationId">alibaba</Put>
                  <Put name="repositoryId">default</Put>
                </New>
              </Set>
            </New>
          </Arg>
          <Arg>/</Arg>
        </Call>
        <Set name="ContextPath">/alibaba</Set>
      </New>
    </Arg>
  </Call>
                                                                        
  <Call name="addHandler">
    <Arg>
      <New class="org.mortbay.jetty.servlet.Context">
        <Call name="addServlet">
          <Arg>
            <New class="org.mortbay.jetty.servlet.ServletHolder">
              <Set name="Servlet">
                <New class="org.openrdf.alibaba.servlet.impl.ImportExportServlet"/>
              </Set>
              <Set name="InitParameters">
                <New class="java.util.HashMap">
                  <Put name="applicationId">alibaba</Put>
                  <Put name="repositoryId">default</Put>
                </New>
              </Set>
            </New>
          </Arg>
          <Arg>/</Arg>
        </Call>
        <Set name="ContextPath">/repository</Set>
      </New>
    </Arg>
  </Call>
                                                                        
</Configure>