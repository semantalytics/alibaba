<?xml version="1.0"?>

<rdf:RDF xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
         xmlns:chrome="http://www.mozilla.org/rdf/chrome#">
    
    <!-- list all the packages being supplied -->
    <rdf:Seq rdf:about="urn:mozilla:package:root">
        <rdf:li rdf:resource="urn:mozilla:package:alibaba"/>
    </rdf:Seq>

    <!-- describe the package -->
	<rdf:Description rdf:about="urn:mozilla:package:alibaba"
		chrome:displayName="Alibaba Firefox Extension"
		chrome:author="openRDF"
		chrome:authorURL="http://www.openrdf.org/"
		chrome:name="alibaba"
		chrome:extension="true"
		chrome:description="">
	</rdf:Description>
        
    <!-- overlay parentnodes -->
    <rdf:Seq rdf:about="urn:mozilla:overlays">
        <rdf:li resource="chrome://browser/content/browser.xul" />
    </rdf:Seq>
    
    <!-- overlay children -->
    <rdf:Seq rdf:about="chrome://browser/content/browser.xul">
        <rdf:li>chrome://alibaba/content/jetty-server.xul</rdf:li>
    </rdf:Seq>
</rdf:RDF>
