		<owl:versionInfo>0.1</owl:versionInfo>
	</owl:Ontology>

	<!-- Classes -->

	<owl:Class rdf:ID="Message">
		<rdfs:label xml:lang="en">Message</rdfs:label>
		<rdfs:comment xml:lang="en">The base class for all messages.</rdfs:comment>
		<rdfs:isDefinedBy rdf:resource="" />
	</owl:Class>

	<owl:Class rdf:ID="invoke">
		<rdfs:label xml:lang="en">invoke</rdfs:label>
		<rdfs:comment xml:lang="en">Send this message to its target.</rdfs:comment>
		<rdfs:isDefinedBy rdf:resource="" />
		<rdfs:subClassOf rdf:resource="&elmo;Message" />
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&elmo;target" />
				<owl:allValuesFrom rdf:resource="&elmo;Message" />
			</owl:Restriction>
		</rdfs:subClassOf>
	</owl:Class>

	<!-- Properties -->

	<owl:ObjectProperty rdf:ID="method">
		<rdfs:label xml:lang="en">method</rdfs:label>
		<rdfs:comment xml:lang="en">The base property for methods of code that handle message.</rdfs:comment>
		<rdfs:isDefinedBy rdf:resource="" />
		<rdfs:range rdf:resource="#Message" />
	</owl:ObjectProperty>

	<!-- Message Properties -->

	<owl:ObjectProperty rdf:ID="target">
		<rdf:type rdf:resource="&owl;FunctionalProperty" />
		<rdfs:label xml:lang="en">target</rdfs:label>
		<rdfs:comment xml:lang="en">The receiver of this message.</rdfs:comment>
		<rdfs:isDefinedBy rdf:resource="" />
		<rdfs:domain rdf:resource="#Message" />
	</owl:ObjectProperty>

	<owl:ObjectProperty rdf:ID="objectResponse">
		<rdfs:label xml:lang="en">object response</rdfs:label>
		<rdfs:comment xml:lang="en">The return value of this message.</rdfs:comment>
		<rdfs:isDefinedBy rdf:resource="" />
		<rdfs:domain rdf:resource="#Message" />
	</owl:ObjectProperty>

	<owl:DatatypeProperty rdf:ID="literalResponse">
		<rdfs:label xml:lang="en">literal response</rdfs:label>
		<rdfs:comment xml:lang="en">The return value of this message.</rdfs:comment>
		<rdfs:isDefinedBy rdf:resource="" />
		<rdfs:domain rdf:resource="#Message" />
	</owl:DatatypeProperty>

	<!-- Method Properties -->

	<owl:AnnotationProperty rdf:ID="imports">
		<rdfs:label xml:lang="en">imports</rdfs:label>
		<rdfs:comment xml:lang="en">A Class that maybe referenced locally in this method.</rdfs:comment>
		<rdfs:isDefinedBy rdf:resource="" />
	</owl:AnnotationProperty>

	<owl:AnnotationProperty rdf:ID="java">
		<rdfs:label xml:lang="en">java</rdfs:label>
		<rdfs:comment xml:lang="en">The block of Java code for this method.</rdfs:comment>
