<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE rdf:RDF [
   <!ENTITY obj  "http://www.openrdf.org/rdf/2009/03/object#" >
   <!ENTITY xsd  "http://www.w3.org/2001/XMLSchema#" >
 ]>
<rdf:RDF
	xmlns:obj="&obj;"
	xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
	xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
	xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
	xmlns:owl="http://www.w3.org/2002/07/owl#"
	xmlns:local="http://example.org/local#"
	xml:base="http://example.org/local">

	<owl:AnnotationProperty rdf:about="&obj;localized"/>

	<owl:Class rdf:about="http://example.org/local#Document"/>

	<owl:DatatypeProperty rdf:ID="title">
		<obj:localized rdf:datatype="&xsd;boolean">true</obj:localized>
		<rdfs:domain rdf:resource="#Document"/>
	</owl:DatatypeProperty>

	<owl:DatatypeProperty rdf:ID="subject">
		<obj:localized rdf:datatype="&xsd;boolean">false</obj:localized>
		<rdfs:domain rdf:resource="#Document"/>
	</owl:DatatypeProperty>

</rdf:RDF>
