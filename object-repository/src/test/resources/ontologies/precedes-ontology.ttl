@prefix obj  :<http://www.openrdf.org/rdf/2009/object#>.
@prefix owl  :<http://www.w3.org/2002/07/owl#>.
@prefix rdfs :<http://www.w3.org/2000/01/rdf-schema#>.
@prefix rdf  :<http://www.w3.org/1999/02/22-rdf-syntax-ns#>.
@prefix xsd  :<http://www.w3.org/2001/XMLSchema#>.

@prefix :<urn:test:precedes#>.

:hello rdfs:subClassOf obj:Message;
  rdfs:subClassOf [owl:onProperty obj:target;
    owl:allValuesFrom :Greeter];
  rdfs:subClassOf [owl:onProperty obj:functionalLiteralResponse;
    owl:allValuesFrom xsd:string].

:hello-john owl:intersectionOf ( :hello [owl:onProperty obj:target; owl:allValuesFrom [owl:oneOf (:john)]]);
  obj:precedes :world;
  obj:precedes :wide;
  obj:java """
  return "john";
""".

# FIXME this should be inferred
:john a :Greeter .

:wide owl:intersectionOf (:hello);
  obj:java """
  return "wide world";
""".

:world owl:intersectionOf (:hello);
  obj:precedes :wide;
  obj:java """
  return "world";
""".

:hello-jim owl:intersectionOf ( :hello [owl:onProperty obj:target; owl:allValuesFrom [owl:oneOf (:jim)]]);
  obj:precedes :world;
  obj:precedes :wide;
  obj:java """
  return msg.getObjFunctionalLiteralResponse() + " jim";
""".

# FIXME this should be inferred
:jim a :Greeter .

