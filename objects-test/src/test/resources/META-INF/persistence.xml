<?xml version="1.0" encoding="UTF-8"?>
<persistence xmlns="http://java.sun.com/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://java.sun.com/xml/ns/persistence http://java.sun.com/xml/ns/persistence/persistence_1_0.xsd"
  version="1.0">

  <persistence-unit name="test">
    <provider>org.openrdf.elmo.sesame.SesamePersistenceProvider</provider>
    <jar-file>testperson.jar</jar-file>
    <class>test.TestPerson</class>
    <exclude-unlisted-classes>true</exclude-unlisted-classes>
    <properties>
      <property name="applicationId" value="elmo-spring-test" />
      <property name="repositoryId" value="test" />
      <property name="resources" value="testcases/sesame-foaf.rdf" />
    </properties>
  </persistence-unit>
</persistence>
